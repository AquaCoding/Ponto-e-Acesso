<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aquacoding.controller.FeriasNovoController">
   <children>
      <Label layoutX="34.0" layoutY="34.0" text="Nova Férias" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="15.0">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <VBox layoutX="10.0" layoutY="10.0" prefHeight="200.0" prefWidth="100.0" spacing="5.0" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="45.0">
         <children>
            <Label text="Nome" />
            <TextField fx:id="feriasNome" VBox.vgrow="ALWAYS" />
            <Label text="Inicio" />
            <AnchorPane prefWidth="200.0">
               <children>
                  <DatePicker fx:id="feriasInicio" />
               </children>
            </AnchorPane>
            <Label text="Termino" />
            <AnchorPane prefWidth="200.0">
               <children>
                  <DatePicker fx:id="feriasTermino" />
               </children>
            </AnchorPane>
            <Label text="Funcionários" />
            <ListView fx:id="listFuncionaios" prefHeight="200.0" prefWidth="200.0" />
            <AnchorPane>
               <children>
                  <Button fx:id="cadastrar" mnemonicParsing="false" text="Cadastar" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                  <Button fx:id="cancelar" mnemonicParsing="false" styleClass="cancelar" text="Cancelar" AnchorPane.leftAnchor="0.0" />
               </children>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
